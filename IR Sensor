#include "STM32F4xx.h"
#include "stdio.h"
#include "stdint.h"
#include "IR_Sensor.h"
int main()
{
	RCC->AHB1ENR |=1<<3;
	RCC->AHB1ENR |=1<<0;

	GPIOA->MODER &= ~(1<<0); // switch input 0x00
	GPIOA->MODER &= ~(1<<1);

	GPIOD->MODER &= ~(1<<27); // led output
	GPIOD->MODER |= 1<<26;


	while(1)
	{
		if((GPIOA->IDR) & (1<<0))
		{
		GPIOD->ODR |= 1<<13;
		}
		else
		{
		GPIOD->ODR &= ~(1<<13);
		}

	}
	return 0;
}
